{
    "name": "lodge104/flarum-ext-oauth-auth0",
    "description": "Log in to your Flarum forum with Auth0",
    "keywords": [
        "flarum", "auth0", "sso", "oauth", "login"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Lodge104/flarum-ext-oauth-auth0/issues",
        "source": "https://github.com/Lodge104/flarum-ext-oauth-auth0/"
    },
    "require": {
        "flarum/core": "^1.2.0",
        "fof/oauth": "^1.1.0",
        "lodge104/oauth2-auth0": "^2.4.1"
    },
    "authors": [
        {
            "name": "Nicholas Anderson",
            "homepage": "https://hey.blue"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lodge104\\OAuthAuth0\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Log In With Auth0",
            "category": "feature",
            "icon": {
                "name": "fas fa-users",
                "backgroundColor": "#0072e3",
                "color": "#fff"
            }
        }
    }
}
